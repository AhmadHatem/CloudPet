<p id="notice"><%= notice %></p>

<h1>Listing Foods</h1>

<table>
  <thead>
    <tr>
      <th>Weight</th>
      <th>Leftovers</th>
      <th>Ate</th>
      <th>Date</th>
      <th>Time</th>
      <th>User</th>
      <th>Register</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foods.each do |food| %>
      <tr>
        <td><%= food.weight %></td>
        <td><%= food.leftovers %></td>
        <td><%= food.ate %></td>
        <td><%= food.date %></td>
        <td><%= food.time %></td>
        <td><%= food.User %></td>
        <td><%= food.Register %></td>
        <td><%= link_to 'Show', food %></td>
        <td><%= link_to 'Edit', edit_food_path(food) %></td>
        <td><%= link_to 'Destroy', food, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Food', new_food_path %>
