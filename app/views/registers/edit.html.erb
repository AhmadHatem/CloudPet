<div class="menu">
      
      <!-- Menu icon -->
      <div class="icon-close">
        <img src='close.png'>
      </div>

<!-- Menu -->
      <ul >
     

<li><%= link_to 'Back', registers_path %></li>
      </ul>
    </div>
     <!-- Main body -->
    <div class="jumbotron">

      <div class="icon-menu">
        <i class="fa fa-bars"></i>
        Menu
      </div>
   
<%= form_for :register, url: registers_path,:html => { :multipart => true } do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
   <%= f.label :image %><br>
  <%= f.file_field :image %>
</p>



  <p>
    <%= f.label :breed %><br>
    <%= f.text_field :breed %>
  </p>

 <p>  <%= f.label :Species %><br>
      <%= f.text_field :species %>


</p>

<p>
  <%= f.label :gender %><br>
  <%= f.text_field :gender %>
</p>

<p>

<%= f.label :date_of_birth %><br />
    <%= f.date_select :date_of_birth, :start_year => 1900, :end_year => Date.current.year %>
</p>






 
  <p>
    <%= f.submit %>
  </p>
<% end %>
</div>
